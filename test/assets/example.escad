// This creates a weird twisty extrusion of 5 unioned circles and lots of rounding. Check it out!
$res = 5;
linear_extrude (height = 40, center=true, twist(h) = 35*cos(h*2*pi/60)) {
  union ( r = 8) {
    circle (10);
    translate ([22,0]) circle (10);
    translate ([0,22]) circle (10);
    translate ([-22,0]) circle (10);
    translate ([0,-22]) circle (10);
  }
}
